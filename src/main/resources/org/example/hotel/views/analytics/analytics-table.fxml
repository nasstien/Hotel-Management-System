<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.geometry.Insets?>

<VBox fx:id="vBox" alignment="CENTER" prefHeight="700.0" prefWidth="1200.0" spacing="10" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.hotel.controllers.AnalyticsController">
   <Label fx:id="headingLabel" styleClass="heading" />
   <TableView fx:id="tableView" focusTraversable="false" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="520.0">
      <VBox.margin>
         <Insets left="50.0" right="50.0" />
      </VBox.margin>
   </TableView>
   <Button onAction="#handleBackClick" text="Back" />
</VBox>
